{
  "$schema": "https://mintlify.com/docs.json",
  "name": "Tybrite API Documentation",
  "theme": "mint",
  "colors": {
    "primary": "#0D9373",
    "light": "#07C983",
    "dark": "#0D9373"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "index",
              "quickstart",
              "authentication"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "openapi.yaml",
        "groups": [
          {
            "group": "Reference",
            "pages": [
              "api-reference/introduction"
            ]
          }
        ]
      },
      {
        "tab": "TypeScript SDK",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "sdk/introduction"
            ]
          },
          {
            "group": "Core Commerce",
            "pages": [
              "sdk/api-reference/classes/ProductsService",
              "sdk/api-reference/classes/OrdersService",
              "sdk/api-reference/classes/CustomersService",
              "sdk/api-reference/classes/CartWishlistService"
            ]
          },
          {
            "group": "Discovery",
            "pages": [
              "sdk/api-reference/classes/SearchService",
              "sdk/api-reference/classes/RecommendationsService",
              "sdk/api-reference/classes/TaxonomyService",
              "sdk/api-reference/classes/CmsService"
            ]
          },
          {
            "group": "Checkout & Logistics",
            "pages": [
              "sdk/api-reference/classes/PaymentsService",
              "sdk/api-reference/classes/ShippingService",
              "sdk/api-reference/classes/PricingService",
              "sdk/api-reference/classes/PromotionsService",
              "sdk/api-reference/classes/GiftCardsService"
            ]
          },
          {
            "group": "Account & Support",
            "pages": [
              "sdk/api-reference/classes/AuthenticationService",
              "sdk/api-reference/classes/MessagingService"
            ]
          },
          {
            "group": "Utility",
            "pages": [
              "sdk/api-reference/classes/SystemService"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://app.tybrite.com",
          "icon": "gauge"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/tybrite",
          "icon": "github"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@tybrite.com",
          "icon": "envelope"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Dashboard",
        "href": "https://app.tybrite.com"
      }
